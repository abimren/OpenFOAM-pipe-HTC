FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 11.42;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }
    pipeEntry
    {
        type            omegaWallFunction;
        value           uniform 422;
    }
    pipeFullyDev
    {
        type            omegaWallFunction;
        value           uniform 422;
    }
}


